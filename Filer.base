formulas:
  frequency: duration("Hello")
views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder.contains("Notes")
    order:
      - file.name
      - file.size
      - file.folder
      - formula.frequency
    sort:
      - property: file.size
        direction: ASC
    columnSize:
      file.backlinks: 221
  - type: cards
    name: View
    image: note.cover
    cardSize: 290
    imageFit: contain
    imageAspectRatio: 0.7
