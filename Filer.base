formulas:
  frequency: duration("Hello")
views:
  - type: table
    name: Table
    order:
      - file.name
      - file.size
      - file.folder
      - formula.frequency
    sort: []
    columnSize:
      file.backlinks: 221
  - type: cards
    name: View
    sort:
      - property: file.ctime
        direction: DESC
    image: note.cover
    cardSize: 290
    imageFit: contain
    imageAspectRatio: 0.7
